[Desktop Entry]
Encoding=UTF-8
ServiceTypes=KonqPopupMenu/Plugin,image/*
TryExec=converseen
Type=Service
Actions=converseenImport;
X-KDE-Submenu=Open with Converseen
X-KDE-Submenu[it]=Apri con Converseen
TryExec=convert

[Desktop Action converseenImport]
Name=Import
Name[it]=Importa
Name[x-test]=xxImportaxx
